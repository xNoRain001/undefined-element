{
  "title": "Scroll Area",
  "desc-with-translation": {
    "zh": "ScrollArea 组件能够让你自定义滚动条。",
    "en-ai": "The ScrollArea component allows you to customize the scrollbar."
  },
  "props": [
    {
      "prop": "width",
      "desc-with-translation": {
        "zh": "容器的宽度",
        "en-ai": "Width of container"
      },
      "type": "string",
      "default": "'100%'"
    },
    {
      "prop": "height",
      "desc-with-translation": {
        "zh": "容器的高度",
        "en-ai": "Height of container"
      },
      "type": "string",
      "default": "'100%'"
    },
    {
      "prop": "resize",
      "desc-with-translation": {
        "zh": "当内容变化时是否更新滚动条的大小",
        "en-ai": "Whether to update the scrollbar size when the content changes"
      },
      "type": "boolean",
      "default": "true"
    },
    {
      "prop": "hidden",
      "desc-with-translation": {
        "zh": "是否隐藏滚动条",
        "en-ai": "Whether to hide scroll bars"
      },
      "type": "boolean",
      "default": "false"
    },
    {
      "prop": "persistent",
      "desc-with-translation": {
        "zh": "是否一直显示滚动条",
        "en-ai": "Whether to display scrollbars all the time"
      },
      "type": "boolean",
      "default": "false"
    }
  ],
  "slots": [
    {
      "name": "default",
      "desc-with-translation": {
        "zh": "默认插槽",
        "en-ai": "Default slot"
      }
    }
  ],
