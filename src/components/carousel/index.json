{
  "title": "Carousel",
  "desc-with-translation": {
    "zh": "Carousel 组件可以实现用更少的空间显示更多信息"
  },
  "props": [
    {
      "prop": "loop",
      "desc-with-translation": {
        "zh": "转盘是否开启循环"
      },
      "type": "boolean",
      "default": "false"
    },
    {
      "prop": "autoplay",
      "desc-with-translation": {
        "zh": "转盘是否开启自动播放"
      },
      "type": "number \\| boolean",
      "default": "false"
    },
    {
      "prop": "vertical",
      "desc-with-translation": {
        "zh": "转盘的方向是否设置为垂直"
      },
      "type": "boolean",
      "default": "false"
    },
    {
      "prop": "modelValue",
      "desc-with-translation": {
